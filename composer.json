{
    "name": "maba/webpack-migration-bundle",
    "description": "Bundle to Help Migrating From Assetic to Webpack by maba",
    "keywords": ["webpack", "assets", "assetic", "symfony", "symfony2", "amd", "commonjs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marius Balčytis",
            "email": "marius.balcytis@gmail.com"
        }
    ],
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/cttpar/webpack-bundle.git"
        },
        {
          "type": "vcs",
          "url": "https://github.com/cttpar/twig-template-modification-bundle.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "maba/webpack-bundle": "dev-master",
        "maba/twig-template-modification-bundle": "dev-master",
        "symfony/assetic-bundle": "^2.8",
        "symfony/framework-bundle": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Maba\\Bundle\\WebpackMigrationBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maba\\Tests\\": "test"
        }
    }
}
