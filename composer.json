{
    "name": "cttpar/webpack-migration-bundle",
    "description": "Bundle to Help Migrating From Assetic to Webpack by maba",
    "keywords": ["webpack", "assets", "assetic", "symfony", "symfony2", "amd", "commonjs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marius Balčytis",
            "email": "marius.balcytis@gmail.com"
        }
    ],
    "repositories": [
        {
          "type": "package",
          "package": {
            "name": "cttpar/webpack-bundle",
            "version": "1.0",
            "source": {
              "url": "https://github.com/cttpar/webpack-bundle.git",
              "type": "git",
              "reference": "origin/master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "cttpar/twig-template-modification-bundle",
            "version": "1.0",
            "source": {
              "url": "https://github.com/cttpar/twig-template-modification-bundle.git",
              "type": "git",
              "reference": "origin/master"
            }
          }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "cttpar/webpack-bundle": "1.0",
        "cttpar/twig-template-modification-bundle": "1.0",
        "symfony/assetic-bundle": "^2.8",
        "symfony/framework-bundle": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Maba\\Bundle\\WebpackMigrationBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maba\\Tests\\": "test"
        }
    }
}
